        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {

	// Set the topic type based on the selected Topic
	var adminTypes = ['1', '2', '3', '4', '5', '6', '7'];
	var devTypes = ['10', '11', '12', '13', '14', '15'];
	var topic = current.training_topic.getValue();
	var adminType, devType = false;
	// Other Topic
	if(topic === "20") {
		var adminTypes_other = ['Field', 'Form', 'Dictionary', 'Filter', 'UI', 'Layout'];
		var devTypes_other = ['Test', 'Debug', 'Flow', 'Workflow', 'Scope'];
		var otherTopic = current.other_topic.getValue().toLowerCase();
		
		adminType = adminTypes_other.some(adminType => otherTopic.includes(adminType.toLowerCase()));
		devType = devTypes_other.some(devType => otherTopic.includes(devType.toLowerCase()));
	}
	// admin Type
	if(adminTypes.includes(topic) || adminType) {
		current.training_type.setValue('1');
	}
	// dev Type
	else if(devTypes.includes(topic) || devType) {
		current.training_type.setValue('2');
	}
	gs.info('BR auto set Training Type based on Topic - current.training_topic.getValue() is: ' + topic);
	

})(current, previous);]]></script>
